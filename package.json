{
  "name": "arango-bsd",
  "version": "0.0.1",
  "description": "Sometimes you just need a backseat driver",
  "homepage": "https://distributhor.github.io/arango-bsd/",
  "license": "MIT",
  "keywords": [
    "arango arangodb multimodel graph db"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/distributhor/arango-bsd.git"
  },
  "contributors": [
    "WK <distributhor@fastmail.com>"
  ],
  "files": [
    "LICENSE.md",
    "README.md",
    "/lib/index.d.ts",
    "/lib/index.d.ts.map",
    "/lib/index.js",
    "/lib/index.js.map"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf dist",
    "build": "yarn clean && tsc",
    "test": "jest -c test/jest.config.unit.js --passWithNoTests",
    "integration:test": "jest -c test/jest.config.integration.js",
    "lint": "yarn lint:check",
    "lint:check": "eslint '{src,test}/**/*.{js,ts,json}'",
    "lint:fix": "eslint --fix '{src,test}/**/*.{js,ts,json}'",
    "lint:format": "yarn prettier:fix",
    "prettier:fix": "prettier --ignore-path .prettierignore --write '{src,test}/**/*.{js,ts,json,md}'",
    "prettier:check": "prettier --ignore-path .prettierignore --check '{src,test}/**/*.{js,ts,json,md}'",
    "doc:markdown": "typedoc --out docs/markdown --readme none --mode modules --includeVersion false --excludePrivate true --stripInternal true --hideGenerator true --plugin typedoc-plugin-markdown",
    "doc:html": "typedoc --tsconfig ./tsconfig.json --out docs --plugin typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default && nodetouch docs/.nojekyll",
    "doc": "yarn readme && yarn doc:html",
    "readme": "node_modules/markdown-include/bin/cli.js md/main.conf.json && node_modules/markdown-include/bin/cli.js md/github.conf.json",
    "release": "yarn build && np"
  },
  "dependencies": {
    "arangojs": "^7.1.0",
    "debug": "^4.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/node-cache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.2",
    "markdown-include": "^0.4.3",
    "np": "^7.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "touch": "^3.1.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.0.3",
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
